! This lexicon lists common `emoticons', or `smiley's.
!
! The list is essentially  taken from Wikipedia
! (http://en.wikipedia.org/wiki/List_of_emoticons, 
!  except `eastern emoticons') with minor modifications.
!
! We currently analyze these as 'Ij', but it may be changed to
! <Ij:smiley> or something similar.
!


0%:) Interjection ;
0%:-) Interjection ;
0%:-3 Interjection ;
0%:3 Interjection ;
0%;^) Interjection ;
3%:) Interjection ;
3%:-) Interjection ;
8) Interjection ;
8-0 Interjection ;
8-D Interjection ;
8D Interjection ; %:$ Interjection ; %:%# Interjection ; %:%#%#%#.. Interjection ; %:%< Interjection ; %:%> Interjection ; %:& Interjection ; %:'( Interjection ; %:') Interjection ; %:'-( Interjection ; %:'-) Interjection ; %:( Interjection ; %:) Interjection ; %:* Interjection ; %:-%# Interjection ; %:-%< Interjection ; %:-& Interjection ; %:-( Interjection ; %:-) Interjection ; %:-)) Interjection ; %:-, Interjection ; %:-. Interjection ; %:-/ Interjection ; %:-D Interjection ; %:-P Interjection ; %:-X Interjection ; %:-[ Interjection ; %:-b Interjection ; %:-c Interjection ; %:-o Interjection ; %:-p Interjection ; %:-| Interjection ; %:-|| Interjection ; %:-þ Interjection ; %:-þ Interjection ; %:/ Interjection ; %:3 Interjection ; %:@ Interjection ; %:D Interjection ; %:L Interjection ; %:P Interjection ; %:S Interjection ; %:X Interjection ; %:[ Interjection ; %:\ Interjection ; %:] Interjection ; %:^) Interjection ; %:^* Interjection ; %:b Interjection ; %:c Interjection ; %:c) Interjection ; %:o Interjection ; %:o) Interjection ; %:p Interjection ; %:{ Interjection ; %:| Interjection ; %:} Interjection ; %:þ Interjection ; %:þ Interjection ; %:っ) Interjection ; %:っc Interjection ; %#-) Interjection ; %%) Interjection ; %%-) Interjection ; %*%\0%/%* Interjection ; %;( Interjection ; %;) Interjection ; %;-) Interjection ; %;-] Interjection ; %;] Interjection ; %;^) Interjection ; %;d Interjection ; %<%:-| Interjection ; %<*)))-{ Interjection ; %</3 Interjection ; %<3 Interjection ; %>%:( Interjection ; %>%:) Interjection ; %>%:-%#%#%#.. Interjection ; %>%:-) Interjection ; %>%:/ Interjection ; %>%:P Interjection ; %>%:[ Interjection ; %>%:\ Interjection ; %>%:o Interjection ; %>%;) Interjection ; %>%<%> Interjection ; %>%<(((*%> Interjection ; %>.%< Interjection ; %>@%>--%>-- Interjection ; %>_%>^ Interjection ; %>}%:-) Interjection ;
'}{' Interjection ;
*) Interjection ;
*-) Interjection ;
