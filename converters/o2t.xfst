           {fakir}|
           {yenik}|
           {narkotik}|
           {merhum}|
           {tuhaf}|
           {dindar}|
           {kırık}|
           {tamam}|
           {saçma}|
           {radikal}|
           {makro}|
           {hayran}|
           {ünlü}|
           {meşhur}|
           {layık}|
           {tıpkı}|
           {demokrat}|
           {komik}|
           {yakın}|
           {genç}|
           {liberal}|
           {saf}|
           {pembe}|
           {kör}|
           {yarı}|
           {hasta}|
           {yoksul}|
           {canlı}|
           {yanlış}|
           {merkez}|
           {muhtaç}|
           {profesyonel}|
           {tutuklu}|
           {haberdar}|
