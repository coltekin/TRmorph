        .o. {onuncu} %<Num%> %<ord%> -> {on} %<Num%> %<ord%>

        .o.  l [e|a] r %<Num%> -> %<Num%> %<pl%> %<ca%> %<Adj%> || {on}|{yüz}|{bin}|{milyon} _
        
        .o. %<p3s%> -> 0    || .#. {kendi} %<Prn%:refl%:3s%> _




        .o. {ne} %<Adj%> -> {ne} %<Prn%:qst%> 
        .o. {nasıl} %<Adj%> -> {nasıl} %<Adj%:qst%> 
!
        .o. {türk} %<Adj%>          -> {Türk} %<N%:prop%>       || .#. _
